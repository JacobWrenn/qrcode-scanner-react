{
  "name": "qrcode-scanner-react",
  "description": "A very simple QR scanner for react.",
  "version": "0.2.0",
  "private": false,
  "author": "Jacob Wrenn <jacob@wrenn.me> (https://github.com/JacobWrenn)",
  "repository": {
    "type": "git",
    "url": "https://github.com/JacobWrenn/qrcode-scanner-react.git"
  },
  "main": "dist/Scanner.jsx",
  "types": "dist/Scanner.d.ts",
  "dependencies": {
    "jsqrcode-ts": "^0.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 2 src/components/*.css dist",
    "compile-ts": "tsc --project tsconfig.publish.json",
    "publish:npm": "npm-run-all clean copy-files compile-ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3",
    "web-vitals": "^2.1.4"
  }
}
